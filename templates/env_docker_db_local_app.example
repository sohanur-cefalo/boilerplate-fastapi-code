# Docker DB + Local App Configuration
# This configuration is for running database in Docker and app locally

# Database URL for Docker database (port 54321 to avoid conflicts)
DATABASE_URL=postgresql://fastapi_user:fastapi_password@localhost:54321/fastapi_db

# Application settings
DEBUG=True
PROJECT_NAME=FastAPI Project
VERSION=1.0.0

# Security (for production)
SECRET_KEY=your-secret-key-here
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30
